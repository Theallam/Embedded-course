#include <stdio.h>

void main() {
	int a, b, c;
	printf("please enter a value\r\n");
	printf("please enter b value\r\n");
	printf("please enter c value\r\n");

	scanf_s("%d %d %d", &a, &b, &c);
			if (a > b) {
				if (a > c) {
					printf("a is the biggest number\n");
				}
				else {
					printf("c is the biggest number\n");
				}

	}
			else {
				if (b > a) {
					if (b > c) {
						printf("b is the biggest number\n");

					}
					else {
						printf("c is the biggest number\n");
					}
				}
			}
	
	
}
