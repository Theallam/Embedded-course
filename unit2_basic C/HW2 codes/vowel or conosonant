#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
char letter;

printf("enter letter to be checked \n");
scanf("%c", &letter);
switch (letter) {
case 'e':
case 'E': 
{
	printf("letter is a vowel");
}
		break;
case 'o':
case 'O': 
{
	printf("letter is a vowel");

}
		break;
case 'i':
case 'I': 
{
	printf("letter is a vowel");

}
		break;
case 'u':
case 'U':
{
	printf("letter is a vowel");

}
		break;
case'a':
case'A': 
{
	printf("letter is a vowel");

}
	   break;
default: {
	printf("letter is not a  vowel");
	break;
}
}
}
